#!/usr/bin/env bash
# bin/release <BUILD_DIR>

cat <<EOF
---
config_vars:
  PATH: /app/local/bin:/usr/local/bin:/usr/bin:/bin
  LD_LIBRARY_PATH: /app/local/lib/luarocks/lib
  LUA_CPATH: ./?.so;app/local/lib/luarocks/lib/lua/5.1/?.so
  LUA_PATH: ./?.lua;/app/local/lib/luarocks/share/lua/5.1/?.lua;/app/local/luarocks/lib/share/lua/5.1/?/init.lua
EOF
