---
config_vars:
  PATH: /opt/local/bin:/usr/local/bin:/usr/bin:/bin
  LD_LIBRARY_PATH: /opt/local/lib/luarocks/lib
  LUA_CPATH: ./?.so;opt/local/lib/luarocks/lib/lua/5.1/?.so
  LUA_PATH: ./?.lua;/opt/local/lib/luarocks/share/lua/5.1/?.lua;/opt/local/luarocks/lib/share/lua/5.1/?/init.lua
EOF
